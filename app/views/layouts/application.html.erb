<!DOCTYPE html>
<html>
  <head>

    <title>SuperRestaurant</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if (request.env["PATH_INFO"] =~ /\/admin\/*/).nil? %>
      <%= render "shared/flash" %>
    <% end %>
    <%= yield %>
  </body>
</html>
